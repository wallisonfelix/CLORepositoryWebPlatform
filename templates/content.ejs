<% include ../html/page_header.html %>
<div class="container">
	  <% include ../html/header.html %>

	  <div class="row">
	    <div id="menu" class="col-sm-3">
	    </div>	 

	    <!-- Page Content -->
	    <div class="col-sm-9">
	    	<ol class="breadcrumb">
			  <li>Home</a></li>
			  <li class="active">Pesquisar OAC</li>
			</ol>
			<label for="fileInput" class="control-label col-sm-2">Pesquisar:</label>
			<form action="/pesquisar_oac" role="form" class="form-horizontal" accept-charset="UTF-8">
			<input type="text" style="resize:none; width: 20%" id="message_input" class="form-control"></input>
			<br><br>
			<div class="form-group">
				<div class="col-sm-offset-2 col-sm-10">
						<input type="submit" name="btnBuscar" class="btn btn-info" value="Buscar" class="form-control">
				</div>
			</div>
			</form>
			<br>
			<% for(var i = 0; i < files.length; i++){ %>
				<p><%= files[i].title %></p>
				<ul>
					<% for(var j = 0; i < files.exec.length; i++){ %>
						<li><%= i+1 %>.<a href="/OAC?<%= files[i].exec[j]._id%>&filePath<%= files[i].exec[j].path%>"><%= files[i].exec[j].ext%></a></li>
					<% } %>
				</ul>
			<br><br>
	    </div>
	  </div>

	</div>
<% include ../html/page_footer.html %>